{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "~/*": ["./*"]
    },
    "types": ["astro/client"],
    "typeRoots": ["./node_modules/@types"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
