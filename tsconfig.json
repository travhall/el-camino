{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "~/*": ["./*"]
    },
    "types": [
      "astro/client"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./src/lib/payload"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}