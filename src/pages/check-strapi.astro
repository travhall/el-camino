<script>
  async function checkStrapi() {
    try {
      const res = await fetch("http://localhost:1337/api");
      const data = await res.json();
      console.log("Available endpoints:", data);
    } catch (error) {
      console.error("Error fetching Strapi info:", error);
    }
  }

  checkStrapi();
</script>

<div>Check your browser console for Strapi information</div>
