{
  "name": "el-camino",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "check": "astro check",
    "dev": "rm -rf .astro && sleep 1 && astro dev",
    "preinstall": "npx only-allow pnpm",
    "preview": "astro preview",
    "preview-build": "PREVIEW=true astro build",
    "preview-start": "node dist/server/entry.mjs",
    "preview-local": "pnpm run preview-build && pnpm run preview-start",
    "start": "astro dev",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:report": "playwright show-report",
    "test:e2e:cart": "playwright test e2e/cart-flow.spec.ts",
    "test:e2e:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/netlify": "6.5.11",
    "@iconify-json/uil": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "5.14.1",
    "astro-icon": "1.1.5",
    "astro-seo-metadata": "0.6.0",
    "sharp": "^0.34.4",
    "square": "43.1.0",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.2",
    "web-vitals": "^5.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@astrojs/node": "9.4.4",
    "@playwright/test": "^1.55.1",
    "@types/node": "^24.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "happy-dom": "^19.0.1",
    "only-allow": "^1.2.1",
    "vitest": "^3.2.4"
  }
}
